<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dhi11VM" type="com.asapplication.app.modules.dhi11.data.viewmodel.Dhi11VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnvideocamera"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_7pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearIPhoneXorne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVideocamera"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_videocamera"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_videocamera"/>
                <ImageView android:id="@+id/imageSignal"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_218pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylebluegray_901">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_6pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSixtyOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_89pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_weight="0.8" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_9pxh"
                android:singleLine="true"
                android:text="@{dhi11VM.dhi11Model.txtSixtyOne}"
                style="@style/txtManropesemibold23" tools:text="@string/lbl32"/>
            <ImageView android:id="@+id/imageHome"
                android:layout_height="@dimen/_27pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_72pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_27pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_home_white_a700_27x27"
                tools:ignore="ContentDescription" tools:src="@drawable/img_home_white_a700_27x27"/>
        </LinearLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtSixtyTwo"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:text="@{dhi11VM.dhi11Model.txtSixtyTwo}"
            style="@style/txtManropesemibold23" tools:text="해당 목적지까지 소요시간 계산 결과"/>


        <LinearLayout android:id="@+id/linearBus"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_31pxv"
        android:layout_width="wrap_content"
        android:orientation="horizontal" style="@style/groupStylebluegray_901" android:layout_gravity="center">

            <TextView
                android:id="@+id/txtSixtyThree"
                style="@style/txtManropesemibold23"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40pxh"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:text="현재 버스 정류장 상황" />

            </LinearLayout>



        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtSixtyFour"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            tools:text="출발\n사색의 광장: 인원수 적음\n체육대학 앞: 인원수 보통\n정문: 인원수 많음"
            style="@style/txtManropesemibold23" />

        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtSixtyFive"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            tools:text="도착\n정문: 인원수 많음\n체육대학 앞: 인원수 보통"
            style="@style/txtManropesemibold23" />



    </LinearLayout>
</layout>
